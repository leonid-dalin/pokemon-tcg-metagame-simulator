{
    "typeCheckingMode": "basic",
    "pythonVersion": "3.12",
    "pythonPlatform": "All",
    "exclude": [
        "**/.history/**",
        "**/__pycache__/**",
        "data/input/**",
        "data/matchups/**",
        "output/**",
        "**/README.md",
        "**/docs/**", 
        "**/requirements.txt",
        "**/.gitignore",
        "**/.git/**"
    ],
    "reportUnnecessaryTypeIgnoreComment": true,
    "reportUnnecessaryCast": true,
    "reportMissingTypeStubs": false
}